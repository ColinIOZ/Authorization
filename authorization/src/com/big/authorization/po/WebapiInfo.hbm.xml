<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- the mapping of WebapiInfo class -->
<hibernate-mapping>
	<class name="com.big.authorization.po.WebapiInfo" table="webapi_info" catalog="authorization">
		<id name="id" type="java.lang.String">
			<column name="id" />
		</id>
		<property name="apiName" type="java.lang.String">
			<column name="api_name" length="50" not-null="true" />
		</property>
		<property name="apiUrl" type="java.lang.String">
			<column name="api_url" length="500" not-null="true" />
		</property>
		<property name="apiUsage" type="java.lang.String">
			<column name="api_usage" length="500" />
		</property>
		<property name="addTime" type="java.util.Date">
			<column name="add_time" />
		</property>
		
	</class>
</hibernate-mapping>
